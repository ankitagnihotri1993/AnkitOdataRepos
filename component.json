{
  "title": "Ankit Odaata",
  "description": "elastic.io component for the odata API",
  "credentials": {
    "fields": {
      "UserName": {
        "label": "UserName",
        "required": true,
        "viewClass": "TextFieldWithNoteView"
      },
      "Password": {
        "label": "Password",
        "required": true,
        "viewClass": "PasswordFieldView"
      },
      "endPointUrl": {
        "label": "End Point Url",
        "required": true,
        "viewClass": "TextFieldWithNoteView"
      }
    }
  },
  "triggers": {
    "GetPeopleViaApi": {
      "main": "./lib/triggers/GetPeopleViaApi.js",
      "type": "polling",
      "title": "Get people With Odata"
    }
  },
  "actions": {
    "createPetWithPromise": {
      "main": "./lib/actions/createPetWithPromise.js",
      "title": "Create a Pet With Promise",
      "metadata": {
        "in": "./lib/schemas/createPet.in.json",
        "out": "./lib/schemas/createPet.out.json"
      }
    },
    "createPetWithGenerators": {
      "main": "./lib/actions/createPetWithGenerators.js",
      "title": "Create a Pet With Generators",
      "metadata": {
        "in": "./lib/schemas/createPet.in.json",
        "out": "./lib/schemas/createPet.out.json"
      }
    }
  }
}
