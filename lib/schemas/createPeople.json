{

    "type": "object",
    "title": "The Root Schema",
    "required": [
      "UserName",
      "FirstName",
      "LastName",
      "Emails",
      "AddressInfo"
    ],
    "properties": {
      "UserName": {
        "type": "string",
        "title": "The Username Schema",
        "default": "",
        "examples": [
          "lewisblack"
        ],
        "pattern": "^(.*)$"
      },
      "FirstName": {
        "type": "string",
        "title": "The Firstname Schema",
        "default": "",
        "examples": [
          "Lewis"
        ],
        "pattern": "^(.*)$"
      },
      "LastName": {
        "type": "string",
        "title": "The Lastname Schema",
        "default": "",
        "examples": [
          "Black"
        ],
        "pattern": "^(.*)$"
      },
      "Emails": {
        "type": "array",
        "title": "The Emails Schema",
        "items": {
          "$id": "#/properties/Emails/items",
          "type": "string",
          "title": "The Items Schema",
          "default": "",
          "examples": [
            "lewisblack@example.com"
          ],
          "pattern": "^(.*)$"
        }
      },
      "AddressInfo": {
        "type": "array",
        "title": "The Addressinfo Schema",
        "items": {
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "Address",
            "City"
          ],
          "properties": {
            "Address": {
              "type": "string",
              "title": "The Address Schema",
              "default": "",
              "examples": [
                "187 Suffolk Ln."
              ],
              "pattern": "^(.*)$"
            },
            "City": {
              "type": "object",
              "title": "The City Schema",
              "required": [
                "Name",
                "CountryRegion",
                "Region"
              ],
              "properties": {
                "Name": {
                  "type": "string",
                  "title": "The Name Schema",
                  "default": "",
                  "examples": [
                    "Boise"
                  ],
                  "pattern": "^(.*)$"
                },
                "CountryRegion": {
                  "type": "string",
                  "title": "The Countryregion Schema",
                  "default": "",
                  "examples": [
                    "United States"
                  ],
                  "pattern": "^(.*)$"
                },
                "Region": {
                  "type": "string",
                  "title": "The Region Schema",
                  "default": "",
                  "examples": [
                    "ID"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    }
  }